        <li class="blog-logo">
            <a class="name" href="{{@blog.url}}"><img src="{{@blog.logo}}" alt="Blog Logo" />Cmchap.com</a>
        </li>
        {{!-- <li class="toggle-topbar menu-icon navigation"><a href="#"><span>Menu</span></a></li> --}}
        <li class="toggle-topbar navigation menu"><a href="#">Menu<i class="fa fa-bars"></i></a></li>
    </ul>

      <section class="top-bar-section">
        <!-- Right Nav Section -->
        <ul class="right">
            {{navigation}}
        </ul>
      </section>
    </nav>

    {{! Everything else gets inserted here }}
    {{{body}}}
</div>
